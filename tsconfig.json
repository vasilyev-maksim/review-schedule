{
    "compilerOptions": {
        "jsx": "react",
        "lib": [
            "dom",
            "es6",
        ],
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "strict": true,
    }
}